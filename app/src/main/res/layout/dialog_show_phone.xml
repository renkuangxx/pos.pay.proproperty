<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/rl_dialog"
    android:paddingLeft="@dimen/dime_16dp"
    android:paddingRight="@dimen/dime_16dp"
    android:orientation="vertical">
    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/list_phone"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tv_cancel"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        android:layout_marginBottom="@dimen/dime_40dp"
        android:background="@drawable/app_shape_solid_white_10radius"
        android:text="取消"
        android:textColor="@color/color1C8CC9"
        android:textSize="18sp" />

</LinearLayout>